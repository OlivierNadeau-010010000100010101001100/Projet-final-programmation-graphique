    </Page>
  </ItemGroup>
  <ItemGroup>
    <Page Update="PageGestionUsager.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
  </ItemGroup>
  <ItemGroup>
    <Page Update="PageExportationActivite.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
